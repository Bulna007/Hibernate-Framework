<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.dmloperations.entities">
	<class name="Journey" table="journey" dynamic-update="true" dynamic-insert="true">
		<id name="journeyNo">
			<column name="journey_no"></column>
		</id>
		<property name="source"/>
		<property name="destination"/>
		<property name="hours"/>
		<property name="journeyDate">
			<column name="journey_dt"/>
		</property>
		<property name="commuteMode">
			<column name="commute_mode"/>
		</property>
		<property name="amount"/>
	</class>
</hibernate-mapping>